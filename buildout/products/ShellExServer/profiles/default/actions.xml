<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="desktop_view" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Edit using Enfold Desktop</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/plone_desktop_config.plonecmd</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="desktop_action" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="view_in_browser" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">View in Browser</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object/manage_addProduct/ShellExServer/view_path}</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
